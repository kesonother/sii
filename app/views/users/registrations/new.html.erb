<h2>S'incrire</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  
   <p><%= f.label :last_name,"Nom" %><br />
  <%= f.text_field :last_name %></p>
  
  <p><%= f.label :first_name,"PrÃ©nom" %><br />
  <%= f.text_field :first_name %></p>
  
  <p><%= f.label :email ;"Adresse mail"%><br />
  <%= f.email_field :email %></p>

  <p><%= f.label :password,"Mot de passe"%><br />
  <%= f.password_field :password %></p>

  <p><%= f.label :password_confirmation, "Confirmation du mot de passe" %><br />
  <%= f.password_field :password_confirmation %></p>

  <p><%= f.submit "Inscription" %></p>
<% end %>

